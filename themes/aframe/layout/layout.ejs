<% var projectTitle = config.title + ' – ' + config.subtitle %>
<% var isHome = is_current('index.html') %>
<% var showExamples = page.type === 'examples' %>
<% var isSecondary = page.layout !== 'examples' %>
<!DOCTYPE html>
<html lang="en" data-is-home="<%- isHome %>" data-is-mobile="false" data-supports-vr="false" data-supports-touch="false" data-root-url="<%- config.root %>" data-title="{title} | A-Frame" data-is-spa="<% config.spa %>" data-ga-id="<%- config.google_analytics %>">
  <head>
    <title><%- page.full_title || (isHome ? projectTitle : (page.title ? page.title + ' — ' : '') + config.title) %></title>
    <%- partial('partials/force_https') %>
    <meta charset="utf-8">
    <meta name="robots" content="<%- config.robots %>">
    <meta name="description" content="<%- projectTitle %>">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <meta property="fb:app_id" content="<%- config.fb_app_id %>">
    <meta property="og:title" content="A-Frame">
    <meta property="og:site_name" content="A-Frame">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://aframe.io/">
    <meta property="og:description" content="<%- config.subtitle %>">
    <meta property="og:image" content="http://aframe.io/images/card.png">
    <meta property="og:image:secure_url" content="https://aframe.io/images/card.png">

    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="@aframevr">
    <meta name="twitter:creator" content="@aframevr">
    <meta name="twitter:title" content="<%- config.title %>">
    <meta name="twitter:description" content="<%- config.subtitle %>">
    <meta name="twitter:image" content="https://aframe.io/images/card.png">

    <meta name="web-app-capable" content="yes">
    <meta name="theme-color" content="black">
    <link rel="icon" sizes="192x192" href="<%- url_for('images/aframe-logo-192.png') %>">

    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="apple-touch-icon" href="<%- url_for('images/aframe-logo-152.png') %>">

    <link rel="alternate" type="application/rss+xml" title="Feed" href="<%- url_for(config.feed.path) %>">

    <%- js('js/compat.js') %>

    <% if (showExamples) { %>
      <%- css('css/index') %>
      <% if (!isHome) { %>
        <%- css('css/examples') %>
      <% } %>
    <% } else { %>
      <%- css('css/secondary') %>
    <% } %>

  </head>
  <body data-page-layout="<%- page.layout %>" data-page-type="<%- page.type %>">

    <!--<img src="<%- url_for('images/aframe-logo.png') %>" alt="A-Frame" title="A-Frame">-->

    <div class="container">

      <div class="columns">
        <div class="column column-example">
          <iframe id="example-iframe" class="example-iframe" src="http://localhost:9000/examples/showcase-spheres-and-fog/" allowfullscreen></iframe>
          <div class="example-description content box">
            <h1 class="example-heading">Spheres &amp; Fog</h1>
          </div>

        </div>
        <div id="header" class="column is-4 header">
          <div class="logo">
            <a href="<%- config.root %>" class="logo-link">
              <span class="logo-link-inner">
                <img src="<%- url_for('images/aframe-name-white.svg') %>" alt="<%= config.title %>" title="<%= config.title %>" id="logo-wordmark" class="logo-wordmark">
              </span>
            </a>

            <div class="logo-inner">
              <iframe id="logo-iframe" class="logo-iframe" src="http://localhost:9000/examples/animation-aframe-logo/?ui=false" allowfullscreen seamless></iframe>
            </div>
          </div>
        </div>
      </div>

      <!--
       <section class="hero is-left">
        <div class="hero-content">
          <div class="container">
            <div class="hero-pitch">
              <h1 class="title is-1">Building blocks for the virtual reality web</h1>
              <p class="subtitle">Use markup to create VR experiences that work across desktop, iOS, Android, and the Oculus Rift.</p>
              <p class="hero-buttons is-left"><a href="<%- url_for('docs/') %>" class="button">Get Started</a></p>
            </div>
          </div>
        </div>
        <iframe id="hero-iframe" class="hero-iframe" src="http://localhost:9000/examples/animation-aframe-logo/?ui=false" allowfullscreen></iframe>
      </section>
      -->

    </div>

    <%- js('js/common.js') %>
    <%- js('js/ga.js') %>

    <%- js('js/single-page.js') %>
    <% if (showExamples) { %>
      <%- js('js/examples.js') %>
    <% } %>
    <% if (isSecondary) { %>
      <%- js('js/docs.js') %>
    <% } %>

  </body>
</html>

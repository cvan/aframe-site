<header id="header" class="header">
  <a id="logo" class="logo borderless-links" href="/">
    <span class="logo__wordmark">A-Frame</span>
  </a>
  <iframe id="exampleIframe" class="example__iframe"
          width="100%"
          height="100%"
          src="<%= site.data.examples.home.scene_url %>"
          allowfullscreen="yes" scrolling="no"></iframe>
</header>

<div class="content content--index">
  <div class="aframe-animation">
  </div>
  <div class="copy__wrap">
    <ul class="menu">
      <li class="menu-item"><a href="<%- url_for('docs/') %>">Docs</a></li>
      <li class="menu-item"><a href="<%- url_for('faq/') %>">FAQ</a></li>
      <li class="menu-item"><a href="<%- url_for('blog/') %>">Blog</a></li>
      <li class="menu-item"><a href="<%- url_for('community/') %>">Community</a></li>
      <li class="menu-item"><a href="<%- config.github.aframe.url %>" target="_blank" rel="external">GitHub</a></li>
    </ul>
    <h1 class="slogan">
      Building blocks for the<br>
      virtual reality web
    </h1>
    <%- page.content %>
    <a href="<%- url_for('docs/') %>" class="get-started nav-link<%- page.canonical_path.match(/docs/) ? ' current' : '' %>">
      Get Started
    </a>
    <a class="credits borderless-link" href="http://mozvr.com/" target="_blank" rel="external"></a>
  </div>
</div>

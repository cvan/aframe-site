*,
*:before,
*:after {
  box-sizing: border-box;
}

html {
  background: #fff;
}

body {
  font-family: KlimaWeb, Helvetica Neue, Helvetica, sans-serif;
  padding: 4rem;
}

// Hide BrowserSync notifications.
#__bs_notify__ {
  display: none !important;
}

.hero.is-main {
  background: #24caff;
}

.hero.is-main .hero-content {
  background: rgba(255,255,255,.55);
  padding: 80px 0 40px;
  position: relative;
  z-index: 1;
}

.hero,
.hero-content {
  position: relative;
}

.hero-iframe {
  height: 100%;
  // max-width: 600px;
  // position: absolute;
  // top: -40px;
  bottom: 0;
  right: 0;
  left: 0;
  top: 0;
  // left: -300px;
  width: 100%;
}

.hero-pitch {
  position: relative;
  z-index: 1;
}

.hero-buttons .button {
  background: #ef2d5e;
  border-radius: 0;
  border: 4px solid #ef2d5e;
  color: #fff;
  font-size: 2rem;
  font-weight: 600;
  height: auto;
  line-height: 1;
  padding: .8rem 1.5rem .35rem;
  transition: all .2s ease-in;
  text-transform: uppercase;
}

.hero-buttons .button:hover {
  background: transparent;
  border-color: #333;
  color: #333;
}

.hero.is-main .hero-buttons {
  padding: 40px 0;
}

@media screen and (min-width: 769px) {
  .hero-buttons.is-left {
    -webkit-box-pack: flex-start;
    -webkit-justify-content: flex-start;
    -ms-flex-pack: flex-start;
    justify-content: flex-start;
  }
}

.header {
  // background: #333;
}

.header .title {
  color: #fff;
}

.header-item a:hover {
  color: #ccc;
}

img {
  display: block;
}

iframe {
  border: 0;
  display: block;
  height: 100%;
  width: 100%;
}

.example-iframe {
  height: 60vh;
}

// .column-example {
//   min-height: 60vh;
// }

.example-heading {

}

.box {
  box-shadow: none;
}

.example-description {
  background: #def;
  border-radius: 0;
}

.logo-link {
  background: rgb(239,45,94);
}

.logo-link {
  border-radius: 200%;
  // padding: 10px;
  display: block;
  position: absolute;
  left: -15%;
  right: -15%;
  bottom: 0;
  width: 130%;
  z-index: 1;
}

.logo-wordmark {
  height: 20px;
  margin: 15px 0;
  width: 100%;
}

.logo {
  border-radius: 5px;
  // height: 140px;
  height: 190px;
  overflow: hidden;
  position: relative;
  width: 100%;
}

.logo-inner {
  overflow: hidden;
  // height: 100px;
  height: 150px;
  position: relative;
  width: 100%;
}

.logo-inner,
.logo-iframe {
  // border-radius: 5px 5px 0 0;
}

.logo-iframe {
  background: #24caff;
  border-bottom: #24caff 10px solid;
}

